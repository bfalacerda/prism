module robot

rob_x:[1..5] init 5;
rob_y:[1..5] init 5;

[up] (rob_y<5) ->  1.0:(rob_y' = rob_y + 1);
[down] (rob_y>1) -> 1.0:(rob_y' = rob_y - 1);
[right] (rob_x<5) -> 1.0:(rob_x' = rob_x + 1);
[left] (rob_x>1) -> 1.0:(rob_x' = rob_x - 1);
[noop] true -> true;

endmodule

